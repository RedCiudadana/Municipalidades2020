    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@aternus/csv-to-xlsx": "^1.0.16",
    "autoprefixer": "^9.8.6",
    "bootstrap": "^4.5.2",
    "camelcase": "^6.2.0",
    "camelcase-keys": "^6.2.2",
    "chart.js": "^2.9.4",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "elasticlunr": "^0.9.5",
    "emoji-regex": "^9.0.0",
    "json-node-normalizer": "^1.0.5",
    "luxon": "^1.25.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.2.7",
