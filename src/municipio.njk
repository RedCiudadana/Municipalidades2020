---
pagination:
  data: municipiosData
  size: 1
  addAllPagesToCollections: true
  alias: municipio
permalink: "/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}/index.html"
tags:
  - municipios
---
{% extends "src/_includes/baseToExtend.njk" %}

{% block head %}
{% endblock %}

{% block content %}
  <div id="fb-root"></div>
  <script async defer crossorigin="anonymous" src="https://connect.facebook.net/es_ES/sdk.js#xfbml=1&version=v9.0" nonce="YTtCnNoo"></script>
  <script type="text/javascript" async src="https://platform.twitter.com/widgets.js"></script>
  <style>
    #map-container {
      width: 100%;
      height: 200px;
    }
  </style>
  <div class="container" id="page-container">
    <div class="row" id="basic-info-row">
      <div class="col-12 py-2">
        <a href="/municipios/1">Municipios</a>
        \
        {{ municipio.municipio }}
      </div>
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <a href="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}/index.html" class="col-12 col-md-3 m-0 p-2 bg-primary text-white text-left text-md-center font-weight-bold border-right">
            <img src="/assets/img/general-white.png" height="25px" alt="Casa" class="mr-1">
            Municipalidades
          </a>
          <a href="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}/corporacion.html" class="col-12 col-md-3 m-0 p-2 text-left text-md-center font-weight-bold border-right">
            <img src="/assets/img/coorporacion.png" height="25px" alt="Casa" class="mr-1">
            Corporación municipal
          </a>
          <div class="col d-flex">
            <div class="mx-auto ml-md-auto mr-md-0 my-auto d-flex py-2">
              <!-- Corregir alineación con tweet  -->
              <style>
                .fb-share-button span {
                  vertical-align: top !important;
                }
              </style>
              <div class="fb-share-button my-auto mr-1" data-href="https://labmunicipal.redciudadana.org/" data-layout="button" data-size="small"><a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Flabmunicipal.redciudadana.org%2F&amp;src=sdkpreparse" class="fb-xfbml-parse-ignore">Compartir</a></div>
              <a  class="twitter-share-button my-auto" href="https://twitter.com/intent/tweet?url=https%3A%2F%2Flabmunicipal.redciudadana.org%2F&via=RedxGuate&text=Laboratorio%20Municipal&hashtags=%23LaboratorioMunicipal">
                Tweet
              </a>
            </div>
          </div>
        </div>
      </div>
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 col-lg-4 p-3 p-lg-4 d-flex">
            <img src="/assets/img/mapa.png" alt="Municipalidad de Guatemala" class="m-auto img-fluid">
          </div>
          <div class="col-12 col-lg-4 p-3 p-lg-4 text-left">
            <h3>
              Municipalidad de
              {{ municipio.municipio }}
            </h3>
            <div class="pb-1 border-bottom">
              <p class="m-0 h6">
                Departamento:
                <span class="text-secondary font-weight-normal">{{municipio.departamento}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Alcalde:
                <span class="text-secondary font-weight-normal">{{municipio.alcalde}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Partido Político:
                <span class="text-secondary font-weight-normal">{{municipio.partido}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Fundación:
                <span class="text-secondary font-weight-normal">{{municipio.fundacion}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Teléfonos:
                <span class="text-secondary font-weight-normal">{{municipio.telefono}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1">
              <p class="m-0 h6">
                E-mail:
                <span class="text-secondary font-weight-normal">{{municipio.email}}</span>
              </p>
            </div>
          </div>
          <div class="col-12 col-lg-4 p-3 p-lg-4">
            <h6>
              Contacto
            </h6>
            <ul class="list-unstyled">
              <li>
                <img src="/assets/img/twitter.png" alt="Twitter" height="30px">
                <a class="text-secondary" href="{{municipio.twitter}}">{{municipio.twitter}}</a>
              </li>
              <li>
                <img src="/assets/img/facebook.png" alt="Facebook" height="30px">
                <a class="text-secondary" href="{{municipio.facebook}}">{{municipio.facebook}}</a>
              </li>
            </ul>
            <button class="btn btn-secondary btn-block text-uppercase">
              <img src="/assets/img/download.png" alt="Descargar" height="20px" class="mr-2">
              Descargar .XLSX
            </button>
          </div>
        </div>
      </div>
      <div class="col-12">
        <div class="bg-white border w-100 row m-0 p-0 mb-2 justify-content-center py-3">
          <div class="col-12 pt-2 text-center">
            <h4 class="">
              Elije la tematica de su interes
            </h4>
          </div>
          <div class="col-12 col-lg-4 mb-2">
            <select class="form-control" name="tematica" id="tematica">
              <option value="General">General</option>
              {% for tematica in collections.tematicas %}
                <option value="{{ tematica.data.title }}">{{ tematica.data.title }}</option>
              {% endfor %}
            </select>
          </div>
        </div>
      </div>
    </div>
    <div class="row" id="tematicas-row">
      {# General #}
      <div class="col-12 mb-2" id="tematica-general">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex" id="tematica-general-header">
            <h4 class="text-white m-0 font-weight-normal">
              General
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#general-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="general-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=general" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <!-- <embed id="embed-embed" src="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=general" type="text/html" class="w-100" height="500px"> -->
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-3 p-2 px-3 border">
                  <h3>
                    Poblacion total
                  </h3>
                  <h1 class="mb-0">
                    {{municipio.cuadro10Poblacion.poblacionTotal | toLocaleString}}
                  </h1>
                  <h6>
                    habitantes
                  </h6>
                  <small>
                    Fuente: Instituto Nacional de Estadística (INE) 
                  </small>
                </div>
                <div class="col-12 col-lg-4 p-3 border">
                  <h6>
                    Población total por sexo
                  </h6>
                  <div height="210px" id="chart-general-1"></div>
                  Fuente: Instituto Nacional de Estadística (INE)
                </div>
                <div class="col-12 col-lg-5 p-3 border">
                  <h6>
                    Población total por área
                  </h6>
                  <div height="160px" id="chart-general-2"></div>
                  Fuente: Instituto Nacional de Estadística (INE)
                </div>
                <div class="col-12 col-lg-6 p-3 border">
                  <h3>
                    Desnutrición Crónica Total
                  </h3>
                  <h1 class="mb-0">
                    {{ municipio.desnutricion.cronica2019.cantidad | toLocaleString }}
                  </h1>
                  <h6>
                    casos
                  </h6>
                  <small>Fuente: Ministerio de salud pública y asistencia social (MSPAS) 2019</small>
                </div>
                <div class="col-12 col-lg-6 p-3 border">
                  <h3>
                    Desnutrición Aguda Total
                  </h3>
                  <h1 class="title-aguda-text mb-0">
                    {{municipio.desnutricion.aguda2019.cantidad | toLocaleString}}
                  </h1>
                  <h6>
                    casos
                  </h6>
                  <small>Fuente: Ministerio de salud pública y asistencia social (MSPAS) 2019</small>
                </div>
                <div class="col-12 col-lg-6 p-3 border">
                  <h3>
                    Desempeño Municipal en Matematica 2018
                  </h3>
                  <h1>
                    {{municipio.cuadro5Y2018.matematicaMunicipal | toLocaleString}}%
                  </h1>
                  <small>Fuente: Instituto Nacional de Estadistica (INE)</small>
                </div>
                <div class="col-12 col-lg-6 p-3 border">
                  <h3>
                    Desempeño Municipal en Lectura 2018
                  </h3>
                  <h1>
                    {{municipio.cuadro5Y2018.lecturaMunicipal | toLocaleString}}%
                  </h1>
                  <small>Fuente: Instituto Nacional de Estadistica (INE)</small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {# Gestión Municipal #}
      <div class="col-12 mb-2" id="tematica-gestion-municipal">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex" id="tematica-gestion-municipal-header">
            <h4 class="text-white m-0 font-weight-normal">
              Gestión Municipal
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#gestión-municipal-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="gestión-municipal-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=gestion-municipal" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <!-- <embed id="embed-embed" src="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=gestion-municipal" type="text/html" class="w-100" height="500px"> -->
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-3 p-2 px-3 border">
                  <h3>
                    Índice de Gestión Municipal
                  </h3>
                  <h1>
                    {{municipio.ranking.segeplan2018 | toLocaleString}}
                  </h1>
                  <small>
                    Fuente: Secretaría de Planificación y Programación de la Presidencia (SEGEPLAN) 2018
                  </small>
                </div>
                <div class="col-12 col-lg-9 p-3 border">
                  <h6>
                    Índice de Gestión Municipal
                  </h6>
                  <div height="120px" id="chart-gestion-municipal-1"></div>
                  <small>Fuente: Secretaría de Planificación y Programación de la Presidencia (SEGEPLAN)</small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {# Transparencia #}
      <div class="col-12 mb-2" id="tematica-transparencia">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex" id="tematica-transparencia-header">
            <h4 class="text-white m-0 font-weight-normal">
              Transparencia
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#transparencia-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="transparencia-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=transparencia" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <!-- <embed id="embed-embed" src="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=transparencia" type="text/html" class="w-100" height="500px"> -->
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-3 p-2 px-3 border">
                  <h3>
                    Acceso a la información pública
                  </h3>
                  <h1>
                    {{municipio.aip.aipPromedio}}%
                  </h1>
                  <small>
                    Fuente: Procuraduría de los Derechos Humanos (PDH) 2019
                  </small>
                </div>
                <div class="col-12 col-lg-9 p-3 border">
                  <h6>
                    Índice de Acceso a la Información Pública
                  </h6>
                  <div height="120px" id="chart-transparencia-1"></div>
                  <small>Fuente: Procuraduría de Derechos Humanos (PDH)</small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {# Nutrición / Alimentación #}
      <div class="col-12 mb-2" id="tematica-nutricion-alimentacion">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex" id="tematica-nutricion-alimentacion-header">
            <h4 class="text-white m-0 font-weight-normal">
              Nutrición / Alimentación
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#nutrición-alimentación-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="nutrición-alimentación-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=nutricion-alimentacion" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <!-- <embed id="embed-embed" src="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=nutricion-alimentacion" type="text/html" class="w-100" height="500px"> -->
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-3 p-2 px-3 border">
            <h3>
              Desnutrición Crónica Total
            </h3>
            <h1 class="mb-0">
              {{ municipio.desnutricion.cronica2019.cantidad | toLocaleString }}
            </h1>
            <h6>casos</h6>
            <h3>
              Desnutrición Aguda Total
            </h3>
            <h1 class="title-aguda-text mb-0">
              {{municipio.desnutricion.aguda2019.cantidad | toLocaleString}}
            </h1>
            <h6>
              casos
            </h6>
            <small>
              Fuente: Ministerio de salud pública y asistencia social (MSPAS) 2019
            </small>
          </div>
          <div class="col-12 col-lg-6 p-3 border">
            <h6>
              Numero de Casos de Desnutrición por año
            </h6>
            <div id="chart-nutricion-1"></div>
            <small>
              Fuente: Ministerio de salud pública y asistencia social (MSPAS)
            </small>
          </div>
          <div class="col-12 col-lg-3 p-3 border">
            <h6>
              Casos de Desnutrición Crónica por sexo
            </h6>
            <div height="220px" id="chart-nutricion-2"></div>
            <small>
              Fuente: Ministerio de salud pública y asistencia social (MSPAS)
            </small>
          </div>
          {# break #}
          <div class="col-12 col-lg-12 p-3 border">
            <h6>
              Casos de Desnutrición por Edad
            </h6>
            <div height="140px" id="chart-nutricion-3"></div>
            <small>
              Fuente: Ministerio de salud pública y asistencia social (MSPAS)
            </small>
          </div>
        </div>
      </div>
      {# Pobreza #}
      <div class="col-12 mb-2" id="tematica-pobreza">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex" id="tematica-pobreza-header">
            <h4 class="text-white m-0 font-weight-normal">
              Pobreza
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#pobreza-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="pobreza-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=pobreza" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <!-- <embed id="embed-embed" src="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=pobreza" type="text/html" class="w-100" height="500px"> -->
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-3 p-2 px-3 border">
                  <h5>
                    Índice de Pobreza Multidimensional
                  </h5>
                  <h1>
                    {{municipio.ipm.ipm2014}}
                  </h1>
                  <small>
                    Fuente: Programa de las Naciones Unidas para el Desarrollo (PNUD) 2014
                  </small>
                </div>
                <div class="col-12 col-lg-9 p-3 border">
                  <h6>
                    Índice de pobreza Multidimensional
                  </h6>
                  <div height="100px" id="chart-pobreza-1"></div>
                  <small>
                    Fuente: Programa de las Naciones Unidas para el Desarrollo (PNUD)
                  </small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {# Finanzas #}
      <div class="col-12 mb-2" id="tematica-finanzas">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex" id="tematica-finanzas-header">
            <h4 class="text-white m-0 font-weight-normal">
              Finanzas
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#finanzas-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="finanzas-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=finanzas" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <!-- <embed id="embed-embed" src="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}?embed=true&tematica=finanzas" type="text/html" class="w-100" height="500px"> -->
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-3 p-2 px-3 border">
                  <h3>
                    Presupuesto Municipal 2019
                  </h3>
