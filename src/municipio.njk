---
pagination:
  data: municipiosData
  size: 1
  addAllPagesToCollections: true
  alias: municipio
permalink: "/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}/index.html"
tags:
  - municipios
---
{% extends "src/_includes/baseToExtend.njk" %}

{% block head %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.css" integrity="sha512-/zs32ZEJh+/EO2N1b0PEdoA10JkdC3zJ8L5FTiQu82LR9S/rOQNfQN7U59U9BC12swNeRAz3HSzIL2vpp4fv3w==" crossorigin="anonymous"/>
{% endblock %}

{% block content %}
  <div id="fb-root"></div>
  <script async defer crossorigin="anonymous" src="https://connect.facebook.net/es_ES/sdk.js#xfbml=1&version=v9.0" nonce="YTtCnNoo"></script>
  <script type="text/javascript" async src="https://platform.twitter.com/widgets.js"></script>
  <style>
    #map-container {
      width: 100%;
      height: 200px;
    }
  </style>
  <div class="container">
    <div class="row">
      <div class="col-12 py-2">
        <a href="/municipios/1">Municipios</a>
        \
        {{ municipio.municipio }}
      </div>
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <a href="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}/index.html" class="col-12 col-md-3 m-0 p-2 bg-primary text-white text-left text-md-center font-weight-bold border-right">
            <img src="/assets/img/general-white.png" height="25px" alt="Casa" class="mr-1">
            Municipalidades
          </a>
          <a href="/{{ municipio.departamento | slug }}/{{ municipio.municipio | slug }}/corporacion.html" class="col-12 col-md-3 m-0 p-2 text-left text-md-center font-weight-bold border-right">
            <img src="/assets/img/coorporacion.png" height="25px" alt="Casa" class="mr-1">
            Corporación municipal
          </a>
          <div class="col d-flex">
            <div class="mx-auto ml-md-auto mr-md-0 my-auto d-flex py-2">
              <!-- Corregir alineación con tweet  -->
              <style>
                .fb-share-button span {
                  vertical-align: top !important;
                }
              </style>
              <div class="fb-share-button my-auto mr-1" data-href="https://labmunicipal.redciudadana.org/" data-layout="button" data-size="small"><a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Flabmunicipal.redciudadana.org%2F&amp;src=sdkpreparse" class="fb-xfbml-parse-ignore">Compartir</a></div>
              <a  class="twitter-share-button my-auto" href="https://twitter.com/intent/tweet?url=https%3A%2F%2Flabmunicipal.redciudadana.org%2F&via=RedxGuate&text=Laboratorio%20Municipal&hashtags=%23LaboratorioMunicipal">
                Tweet
              </a>
            </div>
          </div>
        </div>
      </div>
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 col-lg-4 p-3 p-lg-4 d-flex">
            <img src="/assets/img/mapa.png" alt="Municipalidad de Guatemala" class="m-auto img-fluid">
          </div>
          <div class="col-12 col-lg-4 p-3 p-lg-4 text-left">
            <h3>
              Municipalidad de
              {{ municipio.municipio }}
            </h3>
            <div class="pb-1 border-bottom">
              <p class="m-0 h6">
                Departamento:
                <span class="text-secondary font-weight-normal">{{municipio.departamento}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Alcalde:
                <span class="text-secondary font-weight-normal">{{municipio.alcalde}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Partido Político:
                <span class="text-secondary font-weight-normal">{{municipio.partido}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Fundación:
                <span class="text-secondary font-weight-normal">{{municipio.fundacion}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1 border-bottom">
              <p class="m-0 h6">
                Teléfonos:
                <span class="text-secondary font-weight-normal">{{municipio.telefono}}</span>
              </p>
            </div>
            <div class="pt-1 pb-1">
              <p class="m-0 h6">
                E-mail:
                <span class="text-secondary font-weight-normal">{{municipio.email}}</span>
              </p>
            </div>
          </div>
          <div class="col-12 col-lg-4 p-3 p-lg-4">
            <h6>
              Contacto
            </h6>
            <ul class="list-unstyled">
              <li>
                <img src="/assets/img/twitter.png" alt="Twitter" height="30px">
                <a class="text-secondary" href="{{municipio.twitter}}">{{municipio.twitter}}</a>
              </li>
              <li>
                <img src="/assets/img/facebook.png" alt="Facebook" height="30px">
                <a class="text-secondary" href="{{municipio.facebook}}">{{municipio.facebook}}</a>
              </li>
            </ul>
            <button class="btn btn-secondary btn-block text-uppercase">
              <img src="/assets/img/download.png" alt="Descargar" height="20px" class="mr-2">
              Descargar .XLSX
            </button>
          </div>
        </div>
      </div>
      <div class="col-12">
        <div class="bg-white border w-100 row m-0 p-0 mb-2 justify-content-center py-3">
          <div class="col-12 pt-2 text-center">
            <h4 class="">
              Elije la tematica de su interes
            </h4>
          </div>
          <div class="col-12 col-lg-4 mb-2">
            <select class="form-control" name="tematica" id="tematica">
              <option value="General">General</option>
              {% for tematica in collections.tematicas %}
                <option value="{{ tematica.data.title }}">{{ tematica.data.title }}</option>
              {% endfor %}
            </select>
          </div>
        </div>
      </div>
      {# General #}
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex">
            <h4 class="text-white m-0 font-weight-normal" id="titleTematicaContainer">
              General
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#general-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="general-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/embedCharts/general?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <embed id="embed-embed" src="/embedCharts/general?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px">
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-12 p-2 px-3 border">
                  <p>
                    Información en proceso
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {# Gestión Municipal #}
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex">
            <h4 class="text-white m-0 font-weight-normal" id="titleTematicaContainer">
              Gestión Municipal
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#gestión-municipal-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="gestión-municipal-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/embedCharts/gestion-municipal?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <embed id="embed-embed" src="/embedCharts/gestion-municipal?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px">
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-3 p-2 px-3 border">
                  <h3>
                    Indice de Gestión Municipal
                  </h3>
                  <h1>
                    {{municipio.ranking.segeplan2018}}
                    {{municipio.cuadro10Poblacion.poblacionTotal | toLocaleString}}
                  </h1>
                  <small>
                    SEGEPLAN 2018
                  </small>
                </div>
                <div class="col-12 col-lg-9 p-3 border">
                  <h6>
                    Indice de Gestión Municipal - SEGEPLAN 2018
                  </h6>
                  <canvas height="120px" id="chart-gestion-municipal-1"></canvas>
                  <small>Fuente: SEGEPLAN</small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {# Transparencia #}
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex">
            <h4 class="text-white m-0 font-weight-normal" id="titleTematicaContainer">
              Transparencia
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#transparencia-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="transparencia-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/embedCharts/transparencia?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <embed id="embed-embed" src="/embedCharts/transparencia?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px">
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 p-0">
            <div class="w-100">
              <div class="row m-0 p-0">
                <div class="col-12 col-lg-3 p-2 px-3 border">
                  <h3>
                    Acceso a la información pública
                  </h3>
                  <h1>
                    {{municipio.aip.aipPromedio}}%
                  </h1>
                  <small>
                    PDH 2019
                  </small>
                </div>
                <div class="col-12 col-lg-9 p-3 border">
                  <h6>
                    Indice de Acceso a la Información Pública
                  </h6>
                  <canvas height="120px" id="chart-transparencia-1"></canvas>
                  <small>Fuente: Procuraduría de Derechos Humanos (PDH)</small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {# Nutrición / Alimentación #}
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex">
            <h4 class="text-white m-0 font-weight-normal" id="titleTematicaContainer">
              Nutrición / Alimentación
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#nutrición-alimentación-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="nutrición-alimentación-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                      Compartir gráfica
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="bg-light p-1 mb-2">
                      <code id="code-embed">
                        &lt;embed src="https://labmunicipal.redciudadana.org/embedCharts/nutricion-alimentacion?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px"&gt;
                      </code>
                    </div>
                    <embed id="embed-embed" src="/embedCharts/nutricion-alimentacion?municipio={{municipio.idMunicipal}}" type="text/html" class="w-100" height="500px">
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-3 p-2 px-3 border">
            <h3>
              Desnutrición Crónica Total
            </h3>
            <h1 id="title-cronica-text">
            </h1>
            <small>
              Fuente: Ministerio de salud pública y asistencia social (MSPAS)
            </small>
          </div>
          <div class="col-12 col-lg-9 p-3 border">
            <h6>
              Numero de Casos de Desnutrición Crónica por año
            </h6>
            <canvas height="80px" id="chart-nutricion-1"></canvas>
          </div>
          <div class="col-12 col-lg-6 p-3 border">
            <h6>
              Casos de Desnutrición Crónica por sexo
            </h6>
            <canvas height="140px" id="chart-nutricion-2"></canvas>
          </div>
          {# break #}
          <div class="col-12 col-lg-6 p-3 border">
            <h6>
              Casos de Desnutrición Crónica por Edad
            </h6>
            <canvas height="80px" id="chart-desnutricion-1"></canvas>
          </div>
          {# Aguda #}
          <div class="col-12 col-lg-3 p-2 px-3 border">
            <h3>
              Desnutrición Aguda Total
            </h3>
            <h1>
              {{ municipio.cuadro23['totalDeMujeresDe15AñosYMas'] }}
            </h1>
            <small>
              Fuente: Ministerio de salud pública y asistencia social (MSPAS)
            </small>
          </div>
          <div class="col-12 col-lg-9 p-3 border">
            <h6>
              Numero de Casos de Desnutrición Aguda por año
            </h6>
            <canvas height="80px" id="chart-desnutricion-1"></canvas>
          </div>
          <div class="col-12 col-lg-6 p-3 border">
            <h6>
              Casos de Desnutrición Aguda por sexo
            </h6>
            <canvas height="80px" id="chart-desnutricion-2"></canvas>
          </div>
          {# break #}
          <div class="col-12 col-lg-6 p-3 border">
            <h6>
              Casos de Desnutrición Aguda por Edad
            </h6>
            <canvas height="80px" id="chart-desnutricion-1"></canvas>
          </div>
        </div>
      </div>
      {# Pobreza #}
      <div class="col-12 mb-2">
        <div class="bg-white w-100 row m-0 p-0">
          <div class="col-12 bg-secondary py-2 px-3 d-flex">
            <h4 class="text-white m-0 font-weight-normal" id="titleTematicaContainer">
              Pobreza
            </h4>
            <!-- Button trigger modal -->
            <button type="button" class="btn bg-transparent border-0 text-white ml-auto my-auto" data-toggle="modal" data-target="#pobreza-modal">
              Compartir gráfica
            </button>
            <!-- Modal -->
            <div class="modal fade" id="pobreza-modal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog">
                <div class="modal-content">
                  <div class="modal-header">
